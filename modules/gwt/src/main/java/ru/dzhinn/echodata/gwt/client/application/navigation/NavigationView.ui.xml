<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with type=" ru.dzhinn.echodata.gwt.client.resources.EchoDataNavigationMessages" field="messages"/>
    <ui:with field="appResources" type="ru.dzhinn.echodata.gwt.client.resources.AppResources"/>

    <g:StackLayoutPanel unit='EM'>

        <g:stack>
            <g:customHeader size='3'>
                <g:Label text="{messages.patientListHeader}"/>
            </g:customHeader>
            <g:ScrollPanel ui:field="patientListPanel"/>
        </g:stack>
        <g:stack>
            <g:customHeader size='3'>
                <g:Label text="{messages.templateListHeader}"/>
            </g:customHeader>
            <g:ScrollPanel ui:field="templateListPanel"/>
        </g:stack>
    </g:StackLayoutPanel>
</ui:UiBinder>
